  <ItemGroup>
    <ProjectReference Include="..\AeroIF.Domain\AeroIF.Domain.csproj" />
    <ProjectReference Include="..\AeroIF.Repository\AeroIF.Repository.csproj" />
    <ProjectReference Include="..\AeroIF.Service\AeroIF.Service.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
